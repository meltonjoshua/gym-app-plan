{
  "name": "fittracker-pro",
  "version": "1.0.0",
  "description": "A world-class gym and fitness tracking app built with React Native and Expo",
  "main": "apps/mobile/index.ts",
  "scripts": {
    "start": "cd apps/mobile && expo start",
    "android": "cd apps/mobile && expo start --android",
    "ios": "cd apps/mobile && expo start --ios",
    "web": "cd apps/mobile && expo start --web",
    "test": "jest",
    "test:ios": "jest --testPathPattern='ios-.*\\.test\\.js$'",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration --runInBand",
    "test:api": "node tests/api/postman-tests.js",
    "test:performance": "k6 run tests/performance/load-test.js",
    "test:full": "node tests/test-runner.js",
    "test:full-sequential": "node tests/test-runner.js --sequential",
    "services:start": "concurrently \"npm run service:gateway\" \"npm run service:user\" \"npm run service:workout\" \"npm run service:ai\" \"npm run service:notification\"",
    "service:gateway": "cd apps/microservices/api-gateway && npm start",
    "service:user": "cd apps/microservices/user-service && npm start",
    "service:workout": "cd apps/microservices/workout-service && npm start",
    "service:ai": "cd apps/microservices/ai-service && npm start",
    "service:notification": "cd apps/microservices/notification-service && npm start",
    "type-check": "tsc --noEmit --skipLibCheck",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:production": "eas build --profile production",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "deploy": "eas update --branch production",
    "prebuild": "expo prebuild",
    "install:deps": "npm install && cd backend && npm install"
  },
  "keywords": [
    "fitness",
    "gym",
    "workout",
    "health",
    "tracking",
    "react-native",
    "expo",
    "typescript"
  ],
  "author": "Joshua Melton",
  "license": "MIT",
  "dependencies": {
    "@expo/metro-runtime": "~5.0.4",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "@reduxjs/toolkit": "^2.8.2",
    "@stripe/stripe-react-native": "0.45.0",
    "expo": "~53.0.20",
    "expo-auth-session": "^6.2.1",
    "expo-av": "^15.1.7",
    "expo-camera": "^16.1.11",
    "expo-font": "^13.3.2",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "^14.1.5",
    "expo-localization": "~16.1.6",
    "expo-notifications": "^0.31.4",
    "expo-secure-store": "^14.2.3",
    "expo-sensors": "^14.1.4",
    "expo-status-bar": "~2.2.3",
    "i18n-js": "^4.4.3",
    "moment": "^2.30.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-super-grid": "^6.0.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^19.0.0",
    "babel-jest": "^30.0.5",
    "concurrently": "^9.2.0",
    "jest": "~29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "newman": "^6.2.1",
    "react-test-renderer": "19.0.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/meltonjoshua/gym-app-plan.git"
  },
  "bugs": {
    "url": "https://github.com/meltonjoshua/gym-app-plan/issues"
  },
  "homepage": "https://github.com/meltonjoshua/gym-app-plan#readme"
}
